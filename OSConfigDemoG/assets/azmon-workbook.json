            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "HubConfigsCurrent\n| project _ = \"üìÉ\", configId, targetCondition, appliedCount, completeCount, percentComplete = completeCount / appliedCount, notCompleteCount, dataFreshness\n",
              "size": 1,
